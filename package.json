{
  "name": "react-styled-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "babel-plugin-styled-components": "^1.10.6",
    "prop-types": "^15.7.2",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "prepare": "cross-env NODE_ENV=production babel src --out-dir dist --copy-files",
    "watch": "yarn prepare --watch",
    "build": "styleguidist build",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "dev": "styleguidist server",
    "lint": "stylelint src/**/*.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "babel-plugin-styled-components",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "stylelint": {
    "processors": ["stylelint-processor-styled-components"],
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-styled-components"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-react-app-babel-7": "^4.0.2-0",
    "cross-env": "^5.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "react-styleguidist": "^9.1.13",
    "stylelint": "^10.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-processor-styled-components": "^1.8.0"
  }
}
